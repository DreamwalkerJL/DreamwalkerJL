    "@lottiefiles/react-lottie-player": "^3.4.7",
    "@react-three/cannon": "^6.4.0",
    "@react-three/drei": "^9.34.0",
    "@react-three/fiber": "^8.8.9",
    "@react-three/lightmap": "^0.0.8",
    "@react-three/postprocessing": "^2.6.2",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/user-event": "^14.4.3",
    "@tippyjs/react": "^4.2.6",
    "dat.gui": "^0.7.9",
    "framer-motion": "^7.5.2",
    "gsap": "^3.11.3",
    "lamina": "^1.1.23",
    "leva": "^0.9.31",
    "maath": "^0.4.2",
    "math-random": "^2.0.1",
    "mouse-movement": "^0.0.9",
    "postprocessing": "^6.28.7",
    "react": "^18.2.0",
    "react-cursor-position": "^3.0.3",
    "react-dom": "^18.2.0",
    "react-full-screen": "^1.1.1",
    "react-lottie": "^1.2.3",
    "react-lottie-player": "^1.4.3",
    "react-responsive": "^9.0.0",
    "react-three-gui": "^0.4.2",
    "react-use-refs": "^1.0.1",
    "stats.js": "^0.17.0",
    "styled-components": "^5.3.6",
    "tailwindcss": "^3.1.8",
    "three": "^0.145.0",
    "three.meshline": "^1.4.0",
    "vite-plugin-html": "^3.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
